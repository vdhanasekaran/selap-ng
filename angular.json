{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "selapp": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "targets": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/selapp",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/nouislider/nouislider.css",
              // "https://fonts.googleapis.com/css?family=Playfair+Display:400,400i,700",
              // "src/fonts/hkgrotesk/stylesheet.css",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/owl.carousel/assets/owl.carousel.css",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/ekko-lightbox/ekko-lightbox.css",
              // "src/css/style.default.css",
              // "src/css/custom.css",
              // "https://use.fontawesome.com/releases/v5.1.0/css/solid.css",
              // "https://use.fontawesome.com/releases/v5.1.0/css/regular.css",
              // "https://use.fontawesome.com/releases/v5.1.0/css/brands.css",
              // "https://use.fontawesome.com/releases/v5.1.0/css/fontawesome.css"
            ],
            "scripts": [
              // "src/js/custom.js",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/jquery/jquery.min.js",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/popper.js/umd/popper.min.js",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/bootstrap/js/bootstrap.min.js",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/owl.carousel/owl.carousel.js",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/owl.carousel2.thumbs/owl.carousel2.thumbs.min.js",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/nouislider/nouislider.min.js",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/smooth-scroll/smooth-scroll.polyfills.min.js",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/ekko-lightbox/ekko-lightbox.min.js",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/object-fit-images/ofi.min.js",
              // "https://d19m59y37dris4.cloudfront.net/sell/1-2-2/vendor/jquery.cookie/jquery.cookie.js",
              // "src/js/demo.js",
              // "src/js/theme.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "selapp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "selapp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "selapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "selapp-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "targets": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "selapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "selapp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "selapp"
}